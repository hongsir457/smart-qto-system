/* NVIDIA GeForce风格样式 */
.nvidia-container {
  min-height: 100vh;
  background: linear-gradient(135deg, #0a0a0a 0%, #1a1a1a 50%, #000000 100%);
  color: #ffffff;
  font-family: 'NVIDIA Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
  line-height: 1.6;
}

/* 顶部标题栏 */
.nvidia-header {
  background: linear-gradient(90deg, #76b900 0%, #00d4aa 100%);
  padding: 24px 32px;
  box-shadow: 0 8px 32px rgba(118, 185, 0, 0.3);
}

.nvidia-header-content {
  max-width: 1400px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.nvidia-title {
  margin: 0;
  display: flex;
  flex-direction: column;
  gap: 4px;
}

.nvidia-brand {
  font-size: 32px;
  font-weight: 900;
  color: #000000;
  text-transform: uppercase;
  letter-spacing: 2px;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.nvidia-subtitle {
  font-size: 16px;
  font-weight: 400;
  color: #000000;
  opacity: 0.8;
}

.nvidia-header-stats {
  display: flex;
  gap: 32px;
}

.nvidia-stat {
  text-align: center;
  color: #000000;
}

.nvidia-stat-value {
  display: block;
  font-size: 28px;
  font-weight: 700;
  line-height: 1;
}

.nvidia-stat-label {
  display: block;
  font-size: 12px;
  opacity: 0.8;
  text-transform: uppercase;
  letter-spacing: 1px;
}

/* 项目信息卡片 */
.nvidia-project-card {
  background: rgba(255, 255, 255, 0.05);
  backdrop-filter: blur(10px);
  border: 1px solid rgba(118, 185, 0, 0.2);
  border-radius: 12px;
  padding: 24px 32px;
  margin: 24px 32px;
  max-width: 1400px;
  margin-left: auto;
  margin-right: auto;
}

.nvidia-form .ant-form-item-label > label {
  color: #ffffff !important;
  font-weight: 500;
}

.nvidia-input {
  background: rgba(0, 0, 0, 0.4) !important;
  border: 1px solid rgba(118, 185, 0, 0.3) !important;
  border-radius: 6px !important;
  color: #ffffff !important;
}

.nvidia-input:hover {
  border-color: #76b900 !important;
}

.nvidia-input:focus,
.nvidia-input.ant-input-focused {
  border-color: #76b900 !important;
  box-shadow: 0 0 0 2px rgba(118, 185, 0, 0.2) !important;
}

/* 主要内容区域 */
.nvidia-main-content {
  display: grid;
  grid-template-columns: 2fr 1fr;
  gap: 24px;
  padding: 24px 32px;
  max-width: 1400px;
  margin: 0 auto;
  align-items: stretch;
  height: auto;
}

/* 左侧面板 */
.nvidia-left-panel {
  display: flex;
  flex-direction: column;
  gap: 24px;
  height: auto;
}

/* 右侧面板 */
.nvidia-right-panel {
  display: flex;
  flex-direction: column;
  height: auto;
}

/* 区域标题 */
.nvidia-section-header {
  display: flex;
  align-items: center;
  gap: 12px;
  margin-bottom: 16px;
}

.nvidia-section-header h2 {
  margin: 0;
  font-size: 20px;
  font-weight: 600;
  color: #ffffff;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.nvidia-section-icon {
  color: #76b900;
  font-size: 24px;
}

/* 上传区域 */
.nvidia-upload-section {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(118, 185, 0, 0.2);
  border-radius: 12px;
  padding: 24px;
  height: 320px;
  min-height: 320px;
  max-height: 320px;
  flex-shrink: 0;
  overflow: hidden;
}

.nvidia-upload-area {
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.nvidia-dragger {
  background: rgba(0, 0, 0, 0.4) !important;
  border: 2px dashed rgba(118, 185, 0, 0.5) !important;
  border-radius: 12px !important;
  padding: 40px 20px !important;
  transition: all 0.3s ease !important;
}

.nvidia-dragger:hover {
  border-color: #76b900 !important;
  background: rgba(118, 185, 0, 0.05) !important;
}

.nvidia-upload-icon {
  font-size: 48px !important;
  color: #76b900 !important;
  margin-bottom: 16px !important;
}

.nvidia-upload-text {
  font-size: 18px !important;
  font-weight: 600 !important;
  color: #ffffff !important;
  margin-bottom: 8px !important;
}

.nvidia-upload-hint {
  color: rgba(255, 255, 255, 0.6) !important;
  font-size: 14px !important;
}

.nvidia-upload-actions {
  display: flex;
  gap: 12px;
  justify-content: flex-end;
  flex-wrap: wrap;
}

/* 按钮样式 */
.nvidia-btn {
  border-radius: 6px !important;
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 0.5px !important;
  transition: all 0.3s ease !important;
  height: 40px !important;
  padding: 0 20px !important;
}

.nvidia-btn-primary {
  background: linear-gradient(45deg, #76b900, #00d4aa) !important;
  border: none !important;
  color: #000000 !important;
}

.nvidia-btn-primary:hover {
  background: linear-gradient(45deg, #89d400, #00f4c7) !important;
  transform: translateY(-2px);
  box-shadow: 0 8px 25px rgba(118, 185, 0, 0.4);
}

.nvidia-btn-secondary {
  background: rgba(255, 255, 255, 0.1) !important;
  border: 1px solid rgba(255, 255, 255, 0.3) !important;
  color: #ffffff !important;
}

.nvidia-btn-secondary:hover {
  background: rgba(255, 255, 255, 0.2) !important;
  border-color: #76b900 !important;
  color: #76b900 !important;
}

.nvidia-btn-success {
  background: linear-gradient(45deg, #00d4aa, #76b900) !important;
  border: none !important;
  color: #000000 !important;
}

.nvidia-btn-success:hover {
  background: linear-gradient(45deg, #00f4c7, #89d400) !important;
  transform: translateY(-2px);
  box-shadow: 0 8px 25px rgba(0, 212, 170, 0.4);
}

/* 表格区域 */
.nvidia-table-section {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(118, 185, 0, 0.2);
  border-radius: 12px;
  padding: 24px;
  height: 380px;
  min-height: 380px;
  max-height: 380px;
  flex-shrink: 0;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

/* 表格区域内容 */
.nvidia-table-section .ant-table-wrapper {
  flex: 1;
  overflow: hidden;
}

.nvidia-table-section .ant-table {
  height: 100%;
}

.nvidia-table-section .ant-table-container {
  height: calc(100% - 55px);
  overflow-y: auto;
}

.nvidia-table-container {
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.nvidia-table {
  background: transparent !important;
}

.nvidia-table .ant-table {
  background: transparent !important;
  color: #ffffff !important;
}

.nvidia-table .ant-table-thead > tr > th {
  background: rgba(118, 185, 0, 0.1) !important;
  border-bottom: 2px solid #76b900 !important;
  color: #ffffff !important;
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 0.5px !important;
}

.nvidia-table .ant-table-tbody > tr {
  background: rgba(0, 0, 0, 0.2) !important;
  border-bottom: 1px solid rgba(255, 255, 255, 0.1) !important;
}

.nvidia-table .ant-table-tbody > tr:hover {
  background: rgba(118, 185, 0, 0.1) !important;
}

.nvidia-table .ant-table-tbody > tr > td {
  border-bottom: 1px solid rgba(255, 255, 255, 0.1) !important;
  color: #ffffff !important;
}

.nvidia-filename {
  display: flex;
  align-items: center;
  font-weight: 500;
}

.nvidia-tag {
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 0.5px !important;
}

.nvidia-timestamp {
  color: rgba(255, 255, 255, 0.7);
  font-size: 12px;
}

.nvidia-select {
  background: rgba(0, 0, 0, 0.4) !important;
}

.nvidia-select .ant-select-selector {
  background: rgba(0, 0, 0, 0.4) !important;
  border: 1px solid rgba(118, 185, 0, 0.3) !important;
  color: #ffffff !important;
}

.nvidia-action-btn {
  color: #76b900 !important;
  font-weight: 600 !important;
}

.nvidia-action-btn:hover {
  color: #00d4aa !important;
}

.nvidia-pagination {
  display: flex;
  justify-content: center;
  padding-top: 16px;
}

.nvidia-pagination .ant-pagination {
  color: #ffffff !important;
}

.nvidia-pagination .ant-pagination-item {
  background: rgba(255, 255, 255, 0.1) !important;
  border: 1px solid rgba(255, 255, 255, 0.2) !important;
}

.nvidia-pagination .ant-pagination-item a {
  color: #ffffff !important;
}

.nvidia-pagination .ant-pagination-item-active {
  background: #76b900 !important;
  border-color: #76b900 !important;
}

.nvidia-pagination .ant-pagination-item-active a {
  color: #000000 !important;
}

/* 响应式设计 */
@media (max-width: 1200px) {
  .nvidia-main-content {
    grid-template-columns: 1fr;
    gap: 24px;
  }
  
